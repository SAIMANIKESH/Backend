{
  "compilerOptions": {
    "target": "ES6",                                  
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,   
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node", "express"]  
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.js"
  ],
}
